# Server
PORT=3000
NODE_ENV=development

# Domain
BASE_URL=https://calendar.courtside-ai.com

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# Google OAuth
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret
GOOGLE_REDIRECT_URI=https://calendar.courtside-ai.com/auth/google/callback

# Twilio (for SMS fallback)
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Retell AI
RETELL_WEBHOOK_SECRET=your-webhook-secret

# n8n (for complex date parsing)
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/date-parse

# Security
ENCRYPTION_KEY=32-byte-key-for-aes-256-gcm-encryption

# Frontend (for OAuth redirects)
FRONTEND_URL=https://your-frontend-domain.com
